var _current = 0;
var _bannerSize;
function arrowNext() {
    clearInterval(_slideInterval);
    _current++
    if (_current >= _bannerSize) {
        
        _current = 0
    }
    _bannerRotator(_current);
    if ($("#youtubeVideo:visible").length == 1) {
        var video = $("#youtubeVideo:visible").attr("data-url");
        $("#youtubeVideo:visible").attr("src", video);
        clearInterval(_slideInterval);
    } else {
        $("#youtubeVideo").attr("src", "");
        _slideInterval = setInterval(arrowNext, 8000);
    }
    
}
function arrowPrev() {
    clearInterval(_slideInterval);
    _current--
    if (_current == -1) {
        _current = _bannerSize - 1
    }
    _bannerRotator(_current);
    if ($("#youtubeVideo:visible").length == 1) {
        var video = $("#youtubeVideo:visible").attr("data-url");
        $("#youtubeVideo:visible").attr("src", video);
        clearInterval(_slideInterval);
    } else {
        $("#youtubeVideo").attr("src", "");
        _slideInterval = setInterval(arrowNext, 8000);
    }
}

function _bannerRotator(_index) {
    
    $(".banner-rotator .item").hide();
    $(".banner-rotator .item").eq(_index).show();
    $(".banner-rotator .item .banner-bg").hide();
    $(".bannerList li").removeClass("active");
    $(".bannerList li").eq(_index).addClass("active");
    //if ($(".banner-rotator .item.loaded:visible").length == 0) {
    //    GoogleTrack("Homepage Banner", "Show", $(".banner-rotator .item").eq(_index).find("h3").text(), 0, "true");
    //}

    $(".banner-rotator .item").eq(_index).addClass("loaded").find(".banner-bg").fadeIn("slow");

    $(".banner-rotator .item .left h3, .banner-rotator .item .left h2, .banner-rotator .item .left .rotator-button, .banner-rotator .item .left .maliyet-tablosu-btn").css("opacity", 0).css("marginLeft", 80);
   
    $(".banner-rotator .item .left h3").eq(_index).animate({
        opacity: 1,
        marginLeft: 0
    }, 350)
    $(".banner-rotator .item .left h2").eq(_index).animate({
        opacity: 1,
        marginLeft: 0
    }, 450)
    $(".banner-rotator .item .left .maliyet-tablosu-btn").animate({
        opacity: 1,
        marginLeft: 0
    }, 450)
    setTimeout(function () {
        $(".banner-rotator .item .left .rotator-button").eq(_index).animate({
            opacity: 1,
            marginLeft: 0
        }, 350)
    }, 100);
    
}
var clearTablo;
$().ready(function () {
    var video = $("#youtubeVideo:visible").attr("data-url");
    $("#youtubeVideo:visible").attr("src", video);
    $(".banner-rotator .item").eq(0).addClass("loaded");
    $(".tableNew tr").mouseover(function () { $(this).addClass("hoverHighlight") }); $(".tableNew tr").mouseout(function () { $(this).removeClass("hoverHighlight") })

    $("#wrapper .banner-rotator .item  .maliyet-tablosu-btn").hover(function () {
        $("#wrapper .banner-rotator .item  .maliyet-tablosu-bnr").stop(true, true).fadeIn();
        $("#wrapper .banner-rotator").addClass("z-index");
        clearTimeout(clearTablo);
        clearInterval(_slideInterval);
    }, function () {
        $("#wrapper .banner-rotator .item  .maliyet-tablosu-bnr").stop(true, true).fadeOut(function () {
            $("#wrapper .banner-rotator").removeClass("z-index");
        });
        clearTablo = setTimeout(function () {
            $('#bnr-next').click();
        }, 2000)
    });
    if ($("#youtubeVideo:visible").length == 1) {
        clearInterval(_slideInterval);
    }
});



$(document).ready(function () {
    $('.usertext table').addClass('tblData');
    $('.usertext ul').addClass('.list');

});